#
# ac meter model
#
model suns {
  name "ac_meter"
  did 201 "single phase (AN or AB) meter"
  did 202 "split single phase (ABN) meter"
  did 203 "wye-connect three phase (abcn) meter"
  did 204 "delta-connect three phase (abc) meter"
  datapoints {
    A                 { int16.A_SF     u=A    req label="AC Current"    }
    AphA              { int16.A_SF     u=A    req label="AC Current A"  }
    AphB              { int16.A_SF     u=A    req={ 202 203 204 } label="AC Current B"  }
    AphC              { int16.A_SF     u=A    req label="AC Current C"  }
    A_SF              { sunssf                                        }
    PhV               { int16.V_SF     u=V    req label="AC Voltage LN" }
    PhVphA            { int16.V_SF     u=V    req={ 201 202 203 } label="AC Voltage AN" }
    PhVphB            { int16.V_SF     u=V    req={ 202 203 } label="AC Voltage BN" }
    PhVphC            { int16.V_SF     u=V    req={ 203 } label="AC Voltage CN" }
    PPV               { int16.V_SF     u=V    req label="AC Voltage LL" }
    PhVphAB           { int16.V_SF     u=V    req={ 202 203 204 } label="AC Voltage AB" }
    PhVphBC           { int16.V_SF     u=V    req={ 203 204 } label="AC Voltage BC" }
    PhVphCA           { int16.V_SF     u=V    req={ 203 204 } label="AC Voltage CA" }
    V_SF              { sunssf                req }
    Hz                { int16.Hz_SF    u=Hz   req label="AC Frequency" }
    Hz_SF             { sunssf                                        }
    W                 { int16.W_SF     u=W    req label="AC Power" }
    WphA              { int16.W_SF     u=W    label="Phase A AC Power" }
    WphB              { int16.W_SF     u=W    label="Phase B AC Power" }
    WphC              { int16.W_SF     u=W    label="Phase C AC Power" }
    W_SF              { sunssf req }
    VA                { int16.VA_SF    u=VA   label="AC Apparent Power" }
    VAphA             { int16.VA_SF    u=VA   label="Phase A AC Apparent Power" }
    VAphB             { int16.VA_SF    u=VA   label="Phase B AC Apparent Power" }
    VAphC             { int16.VA_SF    u=VA   label="Phase C AC Apparent Power" }
    VA_SF             { sunssf                                        }
    VAR               { int16.VAR_SF   u=var  label="AC Reactive Power" }
    VARphA            { int16.VAR_SF   u=var  label="Phase A AC Reactive Power" }
    VARphB            { int16.VAR_SF   u=var  label="Phase B AC Reactive Power" }
    VARphC            { int16.VAR_SF   u=var  label="Phase C AC Reactive Power" }
    VAR_SF            { sunssf                                        }
    PF                { int16.PF_SF    u=PFPct label="Power Factor" }
    PFphA             { int16.PF_SF    u=PFPct label="Phase A Power Factor" }
    PFphB             { int16.PF_SF    u=PFPct label="Phase B Power Factor" }
    PFphC             { int16.PF_SF    u=PFPct label="Phase C Power Factor" }
    PF_SF             { sunssf                                        }
    TotWhExp          { acc32.TotWh_SF u=Wh   req label="Energy Exported" }
    TotWhExpPhA       { acc32.TotWh_SF u=Wh   label="Phase A Energy Exported" }
    TotWhExpPhB       { acc32.TotWh_SF u=Wh   label="Phase B Energy Exported" }
    TotWhExpPhC       { acc32.TotWh_SF u=Wh   label="Phase C Energy Exported" }
    TotWhImp          { acc32.TotWh_SF u=Wh   req label="Energy Imported" }
    TotWhImpPhA       { acc32.TotWh_SF u=Wh   label="Phase A Energy Imported" }
    TotWhImpPhAB      { acc32.TotWh_SF u=Wh   label="Phase B Energy Imported" }
    TotWhImpPhAC      { acc32.TotWh_SF u=Wh   label="Phase C Energy Imported" }
    TotWh_SF          { sunssf                req                         }
    TotVAhExp         { acc32.TotVAh_SF   u=VAh label="Apparent Energy Exported" }
    TotVAhExpPhA      { acc32.TotVAh_SF   u=VAh label="Phase A Apparent Energy Exported" }
    TotVAhExpPhB      { acc32.TotVAh_SF   u=VAh label="Phase B Apparent Energy Exported" }
    TotVAhExpPhC      { acc32.TotVAh_SF   u=VAh label="Phase C Apparent Energy Exported" }
    TotVAhImp         { acc32.TotVAh_SF   u=VAh label="Apparent Energy Imported" }
    TotVAhImpPhA      { acc32.TotVAh_SF   u=VAh label="Phase A Apparent Energy Imported" }
    TotVAhImpPhB      { acc32.TotVAh_SF   u=VAh label="Phase B Apparent Energy Imported" }
    TotVAhImpPhC      { acc32.TotVAh_SF   u=VAh label="Phase C Apparent Energy Imported" }
    TotVAh_SF         { sunssf                                            }
    TotVArhImpQ1      { acc32.TotVArh_SF u=varh label="Q1: Reactive Energy Imported"  }
    TotVArhImpQ1PhA   { acc32.TotVArh_SF u=varh label="Q1 Phase A: Reactive Energy Imported" }
    TotVArhImpQ1PhB   { acc32.TotVArh_SF u=varh label="Q1 Phase B: Reactive Energy Imported" }
    TotVArhImpQ1PhC   { acc32.TotVArh_SF u=varh label="Q1 Phase C: Reactive Energy Imported" }
    TotVArhImpQ2      { acc32.TotVArh_SF u=varh label="Q2: Reactive Energy Imported"  }
    TotVArhImpQ2PhA   { acc32.TotVArh_SF u=varh label="Q2 Phase A: Reactive Energy Imported" }
    TotVArhImpQ2PhB   { acc32.TotVArh_SF u=varh label="Q2 Phase B: Reactive Energy Imported" }
    TotVArhImpQ2PhC   { acc32.TotVArh_SF u=varh label="Q2 Phase C: Reactive Energy Imported" }
    TotVArhImpQ3      { acc32.TotVArh_SF u=varh label="Q3: Reactive Energy Exported"  }
    TotVArhImpQ3PhA   { acc32.TotVArh_SF u=varh label="Q3 Phase A: Reactive Energy Exported" }
    TotVArhImpQ3PhB   { acc32.TotVArh_SF u=varh label="Q3 Phase B: Reactive Energy Exported" }
    TotVArhImpQ3PhC   { acc32.TotVArh_SF u=varh label="Q3 Phase C: Reactive Energy Exported" }
    TotVArhImpQ4      { acc32.TotVArh_SF u=varh label="Q4: Reactive Energy Exported"  }
    TotVArhImpQ4PhA   { acc32.TotVArh_SF u=varh label="Q4 Phase A: Reactive Energy Imported" }
    TotVArhImpQ4PhB   { acc32.TotVArh_SF u=varh label="Q4 Phase B: Reactive Energy Imported" }
    TotVArhImpQ4PhC   { acc32.TotVArh_SF u=varh label="Q4 Phase C: Reactive Energy Imported" }
    TotVArh_SF        { sunssf                                            }
    Evt               { bitfield32.MeterEvt_BF req label="Meter Event Flags" }
  }

  define MeterEvt_BF {
    M_EVENT_Power_failure       { 2 "Loss of power or phase" }
    M_EVENT_Under_Voltage       { 3 "Voltage below threshold (Phase Loss)" }
    M_EVENT_Low_PF              { 4 "Power Factor below threshold" }
    M_EVENT_Over_Current        { 5 "Current Input over threshold" }
    M_EVENT_Over_Voltage        { 6 "Voltage Input over threshold" }
    M_EVENT_Missing_Sensor      { 7 "Sensor not connected" }
    M_EVENT_Reserved1           { 8 "Reserved for future" }
    M_EVENT_Reserved2           { 9 "Reserved for future" }
    M_EVENT_Reserved3           { 10 "Reserved for future" }
    M_EVENT_Reserved4           { 11 "Reserved for future" }
    M_EVENT_Reserved5           { 12 "Reserved for future" }
    M_EVENT_Reserved6           { 13 "Reserved for future" }
    M_EVENT_Reserved7           { 14 "Reserved for future" }
    M_EVENT_Reserved8           { 15 "Reserved for future" }
    M_EVENT_OEM01               { 16 "OEM 1" }
    M_EVENT_OEM02               { 17 "OEM 2" }
    M_EVENT_OEM03               { 18 "OEM 3" }
    M_EVENT_OEM04               { 19 "OEM 4" }
    M_EVENT_OEM05               { 20 "OEM 5" }
    M_EVENT_OEM06               { 21 "OEM 6" }
    M_EVENT_OEM07               { 22 "OEM 7" }
    M_EVENT_OEM08               { 23 "OEM 8" }
    M_EVENT_OEM09               { 24 "OEM 9" }
    M_EVENT_OEM10               { 25 "OEM 10" }
    M_EVENT_OEM11               { 26 "OEM 11" }
    M_EVENT_OEM12               { 27 "OEM 12" }
    M_EVENT_OEM13               { 28 "OEM 13" }
    M_EVENT_OEM14               { 29 "OEM 14" }
    M_EVENT_OEM15               { 30 "OEM 15" }
  }
}


#
